#lang eopl
(define empty-stack
  (lambda () '())
  )

(define push
  (lambda (n stack)
    (cons n stack)))

(define pop
  (lambda (stack)
    (cdr stack)))

(define top
  (lambda (stack)
    (car stack)))

(define empty-stack?
  (lambda (stack)
    (if (eqv? stack empty-stack)
        #t
        #f)))

(define a '(1 2 3 4))